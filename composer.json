{
    "name": "ovarun/online-shopping-cart-laravel",
    "description": "Laravel Shopping cart - The original code belogs to mindscms/laravelshoppingcart team. i forked it to add support for larvel 11 and make some changes to support my project",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arun",
            "email": "arunov8545@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^10.0|^11.0",
        "illuminate/session": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/events": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7"
    },
    "autoload": {
        "psr-4": {
            "Gloudemans\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ovarun/online-shopping-cart-laravel"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Gloudemans\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Gloudemans\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
